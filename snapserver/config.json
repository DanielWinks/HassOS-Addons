{
  "name": "Snapserver",
  "version": "1.3.17",
  "slug": "snapserver_hassio",
  "description": "Snapserver for Home Assistant",
  "image": "dwinks/snapserver-{arch}",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "host_network": true,
  "ingress": true,
  "ingress_entry": "iris/",
  "auth_api": true,
  "boot": "auto",
  "map": ["config", "share:rw", "ssl"],
  "options": {
    "spotify_user": "",
    "spotify_pw": ""
  },
  "schema": {
    "spotify_user": "str?",
    "spotify_pw": "str?"
  }
}

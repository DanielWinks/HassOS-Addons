{
  "name": "Snapserver",
  "version": "1.6.4",
  "slug": "snapserver_hassio",
  "description": "Snapserver for Home Assistant, with Librespot",
  "image": "dwinks/snapserver-{arch}",
  "arch": ["armv7", "aarch64", "amd64"],
  "startup": "application",
  "host_network": true,
  "host_dbus": true,
  "boot": "auto",
  "options": {
    "Create FIFO in /share?": false,
    "Start Librespot?": true
  },
  "schema": {
    "Create FIFO in /share?": "bool",
    "Start Librespot?": "bool"
  }
}

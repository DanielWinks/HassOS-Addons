{
  "name": "Snapserver",
  "version": "1.3.16",
  "slug": "snapserver_hassio",
  "description": "Snapserver for Home Assistant",
  "image": "dwinks/snapserver-{arch}",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "host_network": false,
  "boot": "auto",
  "map": ["media:rw"],
  "options": {
    "spotify_user": "",
    "spotify_pw": ""
  },
  "schema": {
    "spotify_user": "str?",
    "spotify_pw": "str?"
  },
  "ports": {
    "1704/tcp": 1704,
    "1705/tcp": 1705,
    "1780/tcp": 1780,
    "6600/tcp": 6600
  }
}
